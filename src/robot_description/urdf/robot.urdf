    <child link="antebraco_esquerdo" />
    <axis xyz="0 1 0" />
  </joint>

  <!-- link: esfera_mao_esquerdo -->
  <link name="esfera_mao_esquerdo">
    <inertial>
      <mass value="0.05" />
      <inertia
                ixx="0.000008"
                ixy="0"
                ixz="0"
                iyy="0.000008"
                iyz="0"
                izz="0.000008"
            />
    </inertial>
    <visual>
      <material name="Blue">
