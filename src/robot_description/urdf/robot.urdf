    <axis xyz="0 1 0" />
  </joint>

  <!-- link: cotovelo_direito -->
  <link name="cotovelo_direito">
    <inertial>
      <mass value="0.1" />
      <inertia
                ixx="0.000024"
                ixy="0"
                ixz="0"
                iyy="0.000024"
                iyz="0"
                izz="0.000024"
            />
    </inertial>
    <visual>
      <material name="Blue">
        <color rgba="0 0 1 1" />
      </material>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </collision>
  </link>

  <!-- joint: ombro_direito -> cotovelo_direito -->
  <joint name="ombro_para_cotovelo_direito" type="fixed">
    <origin xyz="0 0 0.20" rpy="3.14 0 0" />
    <parent link="ombro_direito" />
    <child link="cotovelo_direito" />
    <axis xyz="0 1 0" />
  </joint>

  <!-- link: antebraco_direito -->
  <link name="antebraco_direito">
    <inertial>
      <mass value="0.2" />
      <inertia
                ixx="0.001570"
                ixy="0"
                ixz="0"
                iyy="0.001570"
                iyz="0"
                izz="0.000015"
            />
    </inertial>
    <visual>
      <material name="Blue">
        <color rgba="0 0 1 1" />
      </material>
      <geometry>
        <cylinder length="0.25" radius="0.01" />
      </geometry>
      <origin xyz="0 0 -0.125" rpy="0 0 0" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.25" radius="0.01" />
      </geometry>
      <origin xyz="0 0 -0.125" rpy="0 0 0" />
    </collision>
  </link>

  <!-- joint: cotovelo_direito -> antebraco_direito -->
  <joint name="cotovelo_para_antebraco_direito" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="cotovelo_direito" />
    <child link="antebraco_direito" />
    <axis xyz="0 1 0" />
  </joint>

  <!-- link: esfera_mao_direito -->
  <link name="esfera_mao_direito">
    <inertial>
      <mass value="0.05" />
      <inertia
                ixx="0.000008"
                ixy="0"
                ixz="0"
                iyy="0.000008"
                iyz="0"
                izz="0.000008"
            />
    </inertial>
    <visual>
      <material name="Blue">
        <color rgba="0 0 1 1" />
      </material>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </collision>
