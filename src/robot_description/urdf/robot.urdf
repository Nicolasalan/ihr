<?xml version="1.0" ?>
<robot name="robot">
  <!-- link: base_footprint -->
  <link name="base_footprint" />

  <!-- joint: base_footprint -> base_link -->
  <joint name="base_link_joint" type="fixed">
    <origin xyz="0 0 0.35" rpy="0 0 0" />
