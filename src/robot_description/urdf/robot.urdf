  <!-- joint: ombro_esquerdo -> cotovelo_esquerdo -->
  <joint name="ombro_para_cotovelo_esquerdo" type="fixed">
    <origin xyz="0 0 0.20" rpy="3.14 0 0" />
    <parent link="ombro_esquerdo" />
    <child link="cotovelo_esquerdo" />
    <axis xyz="0 1 0" />
  </joint>

  <!-- link: antebraco_esquerdo -->
  <link name="antebraco_esquerdo">
    <inertial>
      <mass value="0.2" />
      <inertia
                ixx="0.001570"
                ixy="0"
                ixz="0"
                iyy="0.001570"
                iyz="0"
                izz="0.000015"
            />
    </inertial>
    <visual>
      <material name="Blue">
        <color rgba="0 0 1 1" />
      </material>
      <geometry>
        <cylinder length="0.25" radius="0.01" />
      </geometry>
      <origin xyz="0 0 -0.125" rpy="0 0 0" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.25" radius="0.01" />
      </geometry>
      <origin xyz="0 0 -0.125" rpy="0 0 0" />
